mqtt:
  host: <%= @mqtt_host %>
  port: <%= @mqtt_port %>
  ssl: :TLSv1
  ca_cert: /etc/mosquitto/ca_certificates/mqtt_ca.crt
  username: <%= @mqtt_username %>
  password: <%= @mqtt_password %>
  topic: <%= @mqtt_topic %>
influxdb:
  database: <%= @influx_database %>
  username: <%= @influx_username %>
  password: <%= @influx_password %>
